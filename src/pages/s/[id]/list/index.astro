/**
 * Redirect /s/[id]/list â†’ /s/[id]/list/cards (default view).
 * The monolithic list page has been split into separate /cards and /list routes.
 */
import { getAllSeriesIds } from '@config/series';

export function getStaticPaths() {
  return getAllSeriesIds().map(id => ({ params: { id } }));
}

const { id } = Astro.params;
return Astro.redirect(`/s/${id}/list/cards`);
